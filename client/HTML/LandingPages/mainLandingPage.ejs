      <%- include("../components/navbar", {notlogin}); %>

        <header>
          <main>
            <div class="left">
              <div class="heading">
                <h1 class="heading__top">
                  pet<span>Paradise</span><img src="/img/mainLandingPage/dog-sketch.png" alt="" class="dog-sketch" />
